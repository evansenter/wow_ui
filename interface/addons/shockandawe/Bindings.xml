<Bindings>
	<Binding name="SHOCKANDAWE_WATER_SHIELD" header="SHOCKANDAWE_TITLE">
		ShockAndAwe:CastWaterShield();
	</Binding>
	<Binding name="SHOCKANDAWE_LIGHTNING_SHIELD">
		ShockAndAwe:CastLightningShield();
	</Binding>
	<Binding name="SHOCKANDAWE_MHWEAPON_REBUFF">
		ShockAndAwe:RebuffWeapons(16);
	</Binding>
	<Binding name="SHOCKANDAWE_OHWEAPON_REBUFF">
		ShockAndAwe:RebuffWeapons(17);
	</Binding>
	<Binding name="SHOCKANDAWE_ENABLE_DISABLE">
		ShockAndAwe:EnableDisable();
	</Binding>
	<Binding name="SHOCKANDAWE_PRIORITYSET_1">
		ShockAndAwe:SelectPrioritySet(1);
	</Binding>
	<Binding name="SHOCKANDAWE_PRIORITYSET_2">
		ShockAndAwe:SelectPrioritySet(2);
	</Binding>
	<Binding name="SHOCKANDAWE_PRIORITYSET_3">
		ShockAndAwe:SelectPrioritySet(3);
	</Binding>
	<Binding name="SHOCKANDAWE_PRIORITYSET_4">
		ShockAndAwe:SelectPrioritySet(4);
	</Binding>
	<Binding name="SHOCKANDAWE_PRIORITYSET_5">
		ShockAndAwe:SelectPrioritySet(5);
	</Binding>
</Bindings>
